{
	"name": "adawolfa/batchtrine",
	"type": "library",
	"description": "Garbage collector for easy batch processing with Doctrine ORM.",
	"license": ["MIT"],
	"authors": [
		{
			"name": "Adam Klvač",
			"homepage": "https://klva.cz",
			"email": "adam@klva.cz"
		}
	],
	"require": {
		"php": ">=8.1",
		"doctrine/orm": "^2.15|^3.0"
	},
	"require-dev": {
		"ext-pdo": "*",
		"ext-pdo_sqlite": "*",
		"fakerphp/faker": "~1.23.1",
		"phpbench/phpbench": "~1.2.15",
		"phpunit/phpunit": "~10.4.2",
		"symfony/cache": "*"
	},
	"autoload": {
		"psr-4": {
			"Adawolfa\\Batchtrine\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\Adawolfa\\": "tests/"
		}
	},
	"extra": {
		"branch-alias": {
			"dev-master": "1.0.x-dev"
		}
	}
}